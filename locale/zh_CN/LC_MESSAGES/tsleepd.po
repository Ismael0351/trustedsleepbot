#: tsleepd.py:187
msgid "Invalid command. Send /help for help."
msgstr "未知命令。请发送 /help 获得帮助。"

#: tsleepd.py:286
msgid ""
"\n"
"    Identify sleep time using rules as follows:\n"
"\n"
"    -24h    0           6   now\n"
"       /=====================\\  <- SELECT\n"
"       .  x-+-----------+----?\n"
"       .    |     x-----+----？\n"
"       .    |           | x--?\n"
"       .  x-+--------x x| xx\n"
"       .  x-+-----------+--x\n"
"       .  xx| x------x x| xx\n"
"       .    | x x-------+-x\n"
"       .  x |    x------+--x\n"
"       .  x | x       x-+----?\n"
"     x .    |           |    ?\n"
"    "
